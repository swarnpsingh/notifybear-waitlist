{
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/oauth/(.*)",
        "dest": "/oauth/$1"
      },
      {
        "src": "/privacy-policy",
        "dest": "/client/build/index.html"
      },
      {
        "src": "/terms",
        "dest": "/client/build/index.html"
      },
      {
        "src": "/profile",
        "dest": "/client/build/index.html"
      },
      {
        "src": "/login",
        "dest": "/client/build/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "X-Content-Type-Options", "value": "nosniff" },
          { "key": "X-Frame-Options", "value": "DENY" }
        ]
      }
    ]
  }
  